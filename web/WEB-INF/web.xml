<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>carController</servlet-name>
        <servlet-class>com.Controller.carController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>custController</servlet-name>
        <servlet-class>com.Controller.custController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookController</servlet-name>
        <servlet-class>com.Controller.bookController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>carController</servlet-name>
        <url-pattern>/carController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>custController</servlet-name>
        <url-pattern>/custController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookController</servlet-name>
        <url-pattern>/bookController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
